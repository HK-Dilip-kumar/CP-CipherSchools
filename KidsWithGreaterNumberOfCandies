#include<bits/stdc++.h>
using namespace std;

class Solution {
public:
vector kidsWithCandies(vector& candies, int extraCandies) {
vector ans(candies.size());
int max;
max=candies[0];
for(int i=1;i<candies.size();i++){
if(max<candies[i]){
max=candies[i];
}
}
for(int i=0;i<candies.size();i++){
if(candies[i]+extraCandies>=max){
ans[i]=true;
}
else{
ans[i]=false;
}
}
return ans;
}
};
